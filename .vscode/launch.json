{
    "version": "0.2.0",
    "configurations": [
        {
        
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Test",
            "cwd": "${workspaceFolder}/RustFirmware",
            "connectUnderReset": true,
            "wireProtocol": "Swd",
            "speed": 4000,
            "chip": "STM32G431CBUx",
            "runtimeExecutable": "probe-rs",
            "runtimeArgs": [
                "dap-server"
            ],
            "flashingConfig": {
                "flashingEnabled": true,
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "./target/thumbv7em-none-eabi/debug/${fileBasenameNoExtension}",
                    "rttEnabled": true,
                    "options": {
                        "env": {
                            "DEFMT_LOG": "debug"
                        }
                    },
                }
            ],
            "env": {
                //!MODIFY (or remove)
                // If you set this variable, check the VSCode console log window for the location of the log file.
                "RUST_LOG": "info"
            },
            "consoleLogLevel": "Info", //Error, Warn, Info, Debug, Trace
        }
    ]
}